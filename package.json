{
  "name": "vue-directives",
  "version": "1.0.0",
  "description": "This package contains some additional vue directives such as v-focus or v-switch",
  "author": "Marlon Wisskirchen",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./dist/*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "build": "unbuild"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "unbuild": "^3.5.0",
    "vue": "^3.5.13"
  }
}
