{
  "name": "@marlonwiss/vue-directives",
  "version": "0.3.4",
  "description": "This package contains some additional vue directives such as v-focus or v-switch",
  "author": "Marlon Wisskirchen",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarlonWiss2212/vue-directives.git"
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "build": "unbuild"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.16.2",
    "@nuxt/test-utils": "^3.17.2",
    "@types/node": "^22.14.0",
    "nuxt": "^3.16.2",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@nuxt/kit": "^3.16.2"
  }
}
