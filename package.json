{
  "name": "@marlonwiss/vue-directives-base",
  "version": "1.0.0",
  "description": "This package contains a vue and nuxt package for vue directives",
  "author": "Marlon Wisskirchen",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarlonWiss2212/vue-directives.git"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
